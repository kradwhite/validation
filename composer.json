{
    "name": "kradwhite/validation",
    "type": "library",
    "description": "Validation",
    "keywords": [
        "kradwhite",
        "validators",
        "validation",
        "checking"
    ],
    "homepage": "https://github.com/kradwhite/validation",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "kradwhite",
            "email": "kradwhite@mail.ru",
            "homepage": "https://github.com/kradwhite",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "ext-ctype": "*"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "kradwhite\\validation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kradwhite\\validation\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./codecept run unit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}